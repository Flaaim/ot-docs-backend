{
  "require": {
    "symfony/console": "^6.4",
    "php-di/php-di": "^7.1",
    "laminas/laminas-config-aggregator": "^1.18",
    "webmozart/assert": "^1.11",
    "yoomoney/yookassa-sdk-php": "^3.10",
    "ramsey/uuid": "^4.9",
    "doctrine/orm": "^3.5",
    "symfony/cache": "^6.4",
    "doctrine/migrations": "^3.9",
    "doctrine/data-fixtures": "^2.1",
    "symfony/mailer": "^6.4",
    "twig/twig": "^3.21",
    "vlucas/phpdotenv": "^5.6",
    "monolog/monolog": "^3.9",
    "slim/psr7": "^1.7",
    "psr/http-server-handler": "^1.0",
    "guzzlehttp/guzzle": "^7.10",
    "slim/slim": "^4.15",
    "symfony/validator": "^6.4",
    "irazasyed/telegram-bot-sdk": "^3.15"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "app": "php bin/app.php --ansi",
    "test": "phpunit --display-warnings",
    "lint": "phplint",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "deployer/deployer": "^7.5",
    "dms/phpunit-arraysubset-asserts": "^0.5.0",
    "overtrue/phplint": "^9.6",
    "squizlabs/php_codesniffer": "^4.0",
    "vimeo/psalm": "^6.14"
  }
}
